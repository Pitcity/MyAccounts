<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.ihortovpinets.myaccounts"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.WRITE_CALENDAR"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".DealsForAccountActivity"
            android:parentActivityName=".MainPageActivity">
            <!--android:label="@string/about_title">-->
        </activity>
        <activity
            android:name=".CreateDealActivity"
            android:parentActivityName=".MainPageActivity">
        </activity>

        <activity
            android:name=".TryItActivity"
            android:parentActivityName=".TryItActivity">
        </activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
    </application>

</manifest>